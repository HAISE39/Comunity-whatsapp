end
local function Antilasm(data) -- dump anti-lasm
local error = "\108\111\99\97\108\32\95\95\32\61\32\34\89\117\104\34"
local spam = (error.."\n")
data = string.dump(load(data:gsub("function %w+%(%)", function (...)
return ({...})[1].."\n" .. spam:rep(math.random(1,10)) .. "\n"
end)), true, false)
return data:gsub("\4\4\0\0\0\89\117\104","\4\0\0\0")
end
local Boolean
for i = 1, 5 do
    local file = loadfile(gg.getFile())
    local func = debug.getinfo(i)
    if (i == 1 and func['what'] ~= 'Java' and func['source'] ~= '=[Java]') or (i == 2 and func['what'] ~= 'Lua' and not string.match(func['source'], '@')) then
        Boolean = true
    end
    if func then
        local bug_1 = debug.getinfo(1)
        local bug_2 = debug.getinfo(debug.getinfo)
        if bug_1.func ~= bug_2.func then
            return gg.processKill(true)
        end
    end
    if Boolean then
        while true do gg.choice({"Hook : " .. i}) end
        return os.exit(1)
    end
end
